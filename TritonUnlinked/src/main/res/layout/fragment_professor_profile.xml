<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="triton.unlinked.ProfessorProfileActivity$PlaceholderFragment">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"      
        android:text="@string/professor_classlist"
        android:id="@+id/prof_desc"
        android:textColor="@color/textbody"
        android:layout_above="@+id/prof_comment_section"
        android:layout_alignParentLeft="true"
        android:layout_marginBottom="49dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"     
        android:text="ratemyprof?"
        android:id="@+id/prof_comment_section"
        android:textColor="@color/textbody"
        android:layout_marginTop="71dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true" />

	<TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/prof_code_section"
        android:textColor="@color/textbody"
        android:layout_marginTop="35dp"
        android:layout_below="@+id/prof_comment_section"
        android:layout_alignParentLeft="true" />

	<TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/prof_title_section"
        android:textColor="@color/textbody"
        android:layout_marginTop="19dp"
        android:layout_below="@+id/prof_code_section"
        android:layout_alignParentLeft="true" />

	<TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/prof_section_section"
        android:textColor="@color/textbody"
        android:layout_marginTop="16dp"
        android:layout_below="@+id/prof_title_section"
        android:layout_alignParentLeft="true" />
		


</RelativeLayout>
